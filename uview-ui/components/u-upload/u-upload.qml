<block qq:if="{{!disabled}}"><view class="u-upload data-v-2f010004"><block qq:for="{{lists}}" qq:for-item="item" qq:for-index="index" qq:key="index"><block qq:if="{{showUploadList}}"><view class="u-list-item u-preview-wrap data-v-2f010004" style="{{'width:'+($root.g0)+';'+('height:'+($root.g1)+';')}}"><block qq:if="{{deletable}}"><view data-event-opts="{{[['tap',[['deleteItem',[index]]]]]}}" class="u-delete-icon data-v-2f010004" style="{{'background:'+(delBgColor)+';'}}" catchtap="__e"><u-icon class="u-icon data-v-2f010004" vue-id="{{'0d339baa-1-'+index}}" name="{{delIcon}}" size="20" color="{{delColor}}" bind:__l="__l"></u-icon></view></block><block qq:if="{{showProgress&&item.progress>0&&!item.error}}"><u-line-progress class="u-progress data-v-2f010004" vue-id="{{'0d339baa-2-'+index}}" show-percent="{{false}}" height="16" percent="{{item.progress}}" bind:__l="__l"></u-line-progress></block><block qq:if="{{item.error}}"><view data-event-opts="{{[['tap',[['retry',[index]]]]]}}" class="u-error-btn data-v-2f010004" catchtap="__e">点击重试</view></block><block qq:if="{{!item.isImage}}"><image class="u-preview-image data-v-2f010004" src="{{item.url||item.path}}" mode="{{imageMode}}" data-event-opts="{{[['tap',[['doPreviewImage',[item.url||item.path,index]]]]]}}" catchtap="__e"></image></block></view></block></block><slot name="file"></slot><scoped-slots-file file="{{lists}}" bind:__l="__l"></scoped-slots-file><block qq:if="{{maxCount>lists.length}}"><view data-event-opts="{{[['tap',[['selectFile',['$event']]]]]}}" style="display:inline-block;" bindtap="__e" class="data-v-2f010004"><slot name="addBtn"></slot><block qq:if="{{!customBtn}}"><view class="u-list-item u-add-wrap data-v-2f010004" style="{{'width:'+($root.g2)+';'+('height:'+($root.g3)+';')}}" hover-class="u-add-wrap__hover" hover-stay-time="150"><u-icon class="u-add-btn data-v-2f010004" vue-id="0d339baa-3" name="plus" size="40" bind:__l="__l"></u-icon><view class="u-add-tips data-v-2f010004">{{uploadText}}</view></view></block></view></block></view></block>